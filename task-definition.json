{
  "family": "di-customer-ui",
  "executionRoleArn": "arn:aws:iam::061988265798:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "di-customer-ui-microservice",
      "image": "061988265798.dkr.ecr.us-west-2.amazonaws.com/di-customer-ui:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096"
}
